{
  "awesomeTypescriptLoaderOptions": {
    "errorsAsWarnings": true
  },
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2016", "dom"],
    "jsx": "react",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "outDir": "./dist",
    "jsx": "react",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "./src/app/**/*",
    "./src/server.ts"
  ]
}
